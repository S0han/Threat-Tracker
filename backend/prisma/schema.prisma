generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Threat {
  id            Int       @id @default(autoincrement())
  host          String
  url           String
  threatType    String    @map("threat_type")
  dateAdded     DateTime  @map("date_added")
}